[SERVICE]
    Flush        1
    Daemon       Off
    Log_Level    trace


[INPUT]
    Name dummy
    Tag dummy.local
    Dummy {"level": "4", "message": "warn"}

[FILTER]
    Name wasm
    Match dummy.*
    WASM_Path filter.wasm
    Function_Name level_mapper
    Accessible_Paths .

[OUTPUT]
    Name  stdout
    Match *